{
	"name": "Instapinboard"
	, "version": "0.4"
	, "manifest_version": 2 
	, "description": "Right click, add pages to Pinboard.in"
	, "content_security_policy": "default-src 'self'; connect-src https://api.pinboard.in "
	, "background": {
		"page": "background.html"
	}
	, "options_page": "settings.html"
	, "permissions": [
		"contextMenus"
		, "tabs"
		, "https://api.pinboard.in/"
	]
	, "icons": {
		"16": "instapinboard_16.png"
		, "48": "instapinboard_48.png"
		, "128": "instapinboard_128.png"
	}
}
